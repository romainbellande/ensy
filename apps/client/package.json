    "@sveltejs/adapter-auto": "latest",
    "@sveltejs/adapter-static": "^2.0.3",
    "@sveltejs/kit": "^1.24.0",
    "autoprefixer": "^10.4.15",
    "daisyui": "^3.6.4",
    "eslint-config-custom": "workspace:*",
    "postcss": "^8.4.29",
    "svelte": "^4.2.0",
    "svelte-check": "^2.7.1",
    "svelte-preprocess": "^4.10.6",
    "tailwindcss": "^3.3.3",
    "tslib": "^2.3.1",
    "typescript": "^4.9.5",
    "vite": "^4.0.0"
