    "codegen": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@auth0/auth0-spa-js": "^2.1.2",
    "@felte/reporter-svelte": "^1.1.11",
    "@felte/validator-yup": "^1.1.3",
    "clsx": "^2.0.0",
    "dayjs": "^1.11.9",
    "felte": "^1.2.12",
    "graphql": "^16.8.0",
    "graphql-client": "^2.0.1",
    "graphql-request": "^6.1.0",
    "graphql-tag": "^2.12.6",
    "slugify": "^1.6.6",
    "svelte-i18n": "^3.7.0",
    "tailwind-merge": "^1.14.0",
    "ui": "workspace:*",
    "browser-toolkit": "workspace:*",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.0",
