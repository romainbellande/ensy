{
  "name": "@ensy/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "serve": "nx serve",
    "serve:client": "nx serve client -- --hostname=0.0.0.0",
    "inspect": "nx serve --inspect --host 0.0.0.0 --port 9229",
    "build": "nx build",
    "test": "nx test",
    "prepare": "husky install",
    "lint:all": "nx run-many --all --target=lint",
    "test:all": "nx run-many --all --target=test",
    "api:run": "npx nx run api:serve:development",
    "api:e2e": "concurrently --kill-others --group --success=first \"npm run api:run\"  \"sleep 10; npm run api:e2e:run\"",
    "api:e2e:run": "npx newman run ./packages/api/postman/tog.postman_collection.json -e ./packages/api/postman/tog.local.postman_environment.json",
    "console:dev": "ts-node -P ./packages/api/tsconfig.app.json -r tsconfig-paths/register packages/api/src/console.ts",
    "console": "node packages/api/dist/console.js",
    "client:codegen": "graphql-codegen --config ./packages/client/codegen.ts"
  },
  "private": true,
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.2",
    "@nx/eslint-plugin": "16.5.1",
    "@nx/jest": "16.5.1",
    "@nx/js": "16.5.1",
    "@nx/linter": "16.5.1",
    "@nx/nest": "^16.5.1",
    "@nx/node": "16.5.1",
    "@nx/webpack": "16.5.1",
    "@nx/workspace": "16.5.1",
    "@types/jest": "^29.4.0",
    "@types/node": "~18.7.1",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "jest": "^29.4.1",
    "jest-environment-node": "^29.4.1",
    "nx": "16.5.1",
    "prettier": "^2.6.2",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "~5.1.3"
  },
  "dependencies": {
    "@apollo/server": "^4.7.5",
    "@nestjs/apollo": "^12.0.7",
    "@nestjs/common": "^10.0.2",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.2",
    "@nestjs/graphql": "^12.0.8",
    "@nestjs/platform-express": "^10.0.2",
    "@nestjs/typeorm": "^10.0.0",
    "@ngneat/falso": "^6.4.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.41.0",
    "@opentelemetry/instrumentation-express": "^0.33.0",
    "@opentelemetry/instrumentation-graphql": "^0.35.0",
    "@opentelemetry/instrumentation-http": "^0.41.0",
    "@opentelemetry/instrumentation-nestjs-core": "^0.33.0",
    "@opentelemetry/resources": "^1.15.0",
    "@opentelemetry/sdk-node": "^0.41.0",
    "@opentelemetry/semantic-conventions": "^1.15.0",
    "@ptc-org/nestjs-query-core": "^3.0.0",
