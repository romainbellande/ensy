    "@types/feather-icons": "^4.29.1",
    "@vitest/ui": "^0.34.6",
    "eslint-config-custom": "workspace:*",
    "jsdom": "^22.1.0",
    "svelte": "^4.2.0",
    "vite": "link:@sveltejs/kit/vite",
    "vitest": "^0.34.6",
    "vitest-dom": "^0.1.1"
  },
  "dependencies": {
    "@felte/reporter-svelte": "^1.1.11",
    "browser-toolkit": "workspace:*",
